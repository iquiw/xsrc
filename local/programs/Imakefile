#define IHaveSubdirs
#define PassCDebugFlags CDEBUGFLAGS="$(CDEBUGFLAGS)"

#ifndef UseFreetype2
#define UseFreetype2	NO
#endif

#if UseFreetype2
        FTSRCDIR = xftlsfonts
#endif

SUBDIRS = x11-ssh-askpass $(FTSRCDIR) xsystrace

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
