FONTINSTDIR = $(FONTDIR)/CID

MakeFontsDir(fonts.scale)

/*
 * Replace the next line with 'InstallFonts($(FONTINSTDIR))' when fonts are
 * added.
 */
InstallTarget(install,fonts.dir,$(INSTDATFLAGS),$(FONTINSTDIR))

InstallFontScale($(FONTINSTDIR))
