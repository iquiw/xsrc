#define IHaveSubdirs
#define PassCDebugFlags /* as nothing */

#if Build75DpiFonts
DPI75DIR = 75dpi
#endif
#if Build100DpiFonts
DPI100DIR = 100dpi
#endif

        SUBDIRS = $(DPI75DIR) $(DPI100DIR)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
