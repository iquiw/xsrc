#include <Server.tmpl>

#define IHaveSubdirs

#if !XieDynamicModule
     SUBDIRS = dixie mixie

MakeSubdirs($(SUBDIRS))
#else
     SUBDIRS = dixie/export dixie/import dixie/process dixie/request \
		mixie/control mixie/export mixie/fax mixie/import \
		mixie/jpeg mixie/process dixie mixie

DONES = \
	dixie/export/DONE \
	dixie/import/DONE \
	dixie/process/DONE \
	dixie/request/DONE \
	mixie/control/DONE \
	mixie/export/DONE \
	mixie/fax/DONE \
	mixie/import/DONE \
	mixie/jpeg/DONE \
	mixie/process/DONE

OBJS = \
	dixie/export/?*.o \
	dixie/import/?*.o \
	dixie/process/?*.o \
	dixie/request/?*.o \
	mixie/control/?*.o \
	mixie/export/?*.o \
	mixie/fax/?*.o \
	mixie/import/?*.o \
	mixie/jpeg/?*.o \
	mixie/process/?*.o

REQUIREDLIBS = -lm

SharedDepModuleTarget(xie.so,$(SUBDIRS) $(DONES),$(OBJS))
InstallDynamicModule(xie.so,$(MODULEDIR))
ForceSubdirs($(SUBDIRS))
#endif

DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
LintSubdirs($(SUBDIRS))
