#include <Server.tmpl>

SRCS = decInit.c \
	decFbs.c \
	decIo.c \
	decCursor.c \
	decKbd.c \
	decKeyMap.c \
	decColormap.c \
	decMouse.c

OFILES = decInit.o

OBJS =	decFbs.o \
	decIo.o \
	decCursor.o \
	decKbd.o \
	decKeyMap.o \
	decColormap.o \
	decMouse.o

   INCLUDES = -I. -I$(SERVERSRC)/mfb -I$(SERVERSRC)/mi -I$(SERVERSRC)/cfb -I$(SERVERSRC)/mi \
	      -I$(SERVERSRC)/include -I$(SERVERSRC)/os \
	      -I$(XINCLUDESRC) -I$(EXTINCSRC)

NormalLibraryObjectRule()

#define IHaveSubdirs

SUBDIRS = sfb cfb px mfb
#if defined(AlphaArchitecture)
SUBDIRS += tga
#endif

all:: $(OFILES)

lintlib:

NormalLibraryTarget(dec,$(OBJS))
NormalLintTarget($(SRCS1))

DependTarget()
MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
