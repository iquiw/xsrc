#include <Server.tmpl>

SRCS = decInit.c \
	decFbs.c \
	decIo.c \
	decCursor.c \
	decKbd.c \
	decWsMouse.c \
	decSerMouse.c \
	decKeyMap.c \
	decColormap.c

OFILES = decInit.o

OBJS =	decFbs.o \
	decIo.o \
	decCursor.o \
	decKbd.o \
	decWsMouse.o \
	decSerMouse.o \
	decKeyMap.o \
	decColormap.o

   INCLUDES = -I. -I$(SERVERSRC)/mfb -I$(SERVERSRC)/mi -I$(SERVERSRC)/cfb -I$(SERVERSRC)/mi \
	      -I$(SERVERSRC)/include -I$(SERVERSRC)/os \
	      -I$(XINCLUDESRC) -I$(EXTINCSRC)

NormalLibraryObjectRule()

#define IHaveSubdirs

SUBDIRS = sfb cfb px
#if defined(AlphaArchitecture)
SUBDIRS += tga
#endif

all:: $(OFILES)

lintlib:

NormalLibraryTarget(dec,$(OBJS))
NormalLintTarget($(SRCS1))

DependTarget()
MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
