#include <Server.tmpl>

SRCS = 	vgaHW.c vga.c w32vars.c et4000.c et4000w32.c ramdac.c

OBJS = 	vgaHW.o vga.o w32vars.o et4000.o et4000w32.o ramdac.o

INCLUDES = -I. -I$(XF68COMSRC) -I$(XF68OSSRC) -I$(XF68HWSRC) \
	   -I$(SERVERSRC)/mfb -I$(SERVERSRC)/mi \
           -I$(SERVERSRC)/include -I$(SERVERSRC)/cfb \
           -I$(XINCLUDESRC) -I$(FONTINCSRC) \
	   -I$(XF68SRC)/vga256/vga -I$(XF68SRC)/vga256/drivers/et4000

#if DirtyStartup
STARTUPDEFINES = -DDIRTY_STARTUP
#endif

DEFINES = $(SPEEDUPDEFINES) $(STARTUPDEFINES) -DPSZ=8 \
	  -DW32_SUPPORT -DW32_ACCEL_SUPPORT -DUSE_PIXMUX -UDPMSExtension

NormalAsmObjectRule()
NormalLibraryObjectRule()
NormalLibraryTarget(w32,$(OBJS))

LinkSourceFile(vgaHW.c,$(XF86SRC)/vga256/vga)
LinkSourceFile(et4000w32.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(et4000.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(gglbox.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(ggldraw.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(ramdac.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(vga.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(vgaCmap.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32blt.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32box.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32funcs.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32itext.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32map.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32stip.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32vars.c,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(w32version.h,$(XF86ACCELSRC)/et4000w32/w32)
LinkSourceFile(vga256.h,$(XF68SRC)/vga256/vga)

InstallLinkKitLibrary(w32,$(LINKKITDIR)/drivers)

DependTarget()
