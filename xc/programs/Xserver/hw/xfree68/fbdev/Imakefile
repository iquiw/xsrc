#include <Server.tmpl>


#ifdef NetBSDArchitecture
SRCS = fbdev_netbsd.c
OBJS = fbdev_netbsd.o
#else
SRCS = fbdev.c
OBJS = fbdev.o
#endif

#if XF68FBDevServer
#if XF68FBDevIPLAN2p2
FB_DEFINES+= -DCONFIG_IPLAN2p2
#endif
#if XF68FBDevIPLAN2p4
FB_DEFINES+= -DCONFIG_IPLAN2p4
#endif
#if XF68FBDevIPLAN2p8
FB_DEFINES+= -DCONFIG_IPLAN2p8
#endif
#if XF68FBDevILBM
FB_DEFINES+= -DCONFIG_ILBM
#endif
#if XF68FBDevAFB
FB_DEFINES+= -DCONFIG_AFB
#endif
#endif
#if XF68FBDevCFB8
FB_DEFINES+= -DCONFIG_CFB8
#endif
#if XF68FBDevCFB16
FB_DEFINES+= -DCONFIG_CFB16
#endif
#if XF68FBDevCFB24
FB_DEFINES+= -DCONFIG_CFB24
#endif
#if XF68FBDevCFB32
FB_DEFINES+= -DCONFIG_CFB32
#endif


INCLUDES = -I../common -I../os-support -I$(XF68COMSRC) -I$(XF68OSSRC) -I. \
	   -I$(SERVERSRC)/mfb -I$(SERVERSRC)/ilbm -I$(SERVERSRC)/afb \
	   -I$(SERVERSRC)/mi -I$(SERVERSRC)/include -I$(XINCLUDESRC)

NormalLibraryObjectRule()
NormalLibraryTarget(fbdev,$(OBJS))
LintLibraryTarget(fbdev,$(SRCS))
NormalLintTarget($(SRCS))
#ifdef NetBSDArchitecture
SpecialCObjectRule(fbdev_netbsd,NullParameter,$(FB_DEFINES))
#else
SpecialCObjectRule(fbdev,NullParameter,$(FB_DEFINES))
#endif

InstallLinkKitLibrary(fbdev,$(LINKKITDIR)/drivers)

DependTarget()
